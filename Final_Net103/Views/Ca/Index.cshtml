@model IEnumerable<Final_Net103.DomainClass.Ca>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>
<p>
    <a asp-action="RoleBack">RoleBack</a>
</p>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ten)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ThucAn)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TapTinh)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdcaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ten)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ThucAn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TapTinh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Idca)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>|
                    <a asp-action="RoleBackEdit" asp-route-id="@item.Id">RoleBackEdit</a>

            </td>
        </tr>
}
    </tbody>
</table>
